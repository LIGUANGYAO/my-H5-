/* pages/my/history/history.wxss */

.over {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  text-align: center;
  color: #999;
}

page {
  height: 100%;
  background: #efeff4 !important;
}

.history-box {
  padding: 24rpx 0;
  height: 100%;
  box-sizing: border-box;
}
.rows {
  margin-bottom: 80rpx;
}
.card {
  display: block;
  width: 650rpx;
  height: 300rpx;
  margin: 0 auto 48rpx auto;
  position: relative;
  background: #fff;
  padding: 0 22rpx;
}

.card::after {
  content: '.';
  text-indent: -999px;
  background: url(data:image/png;base64,) left bottom repeat-x;
  background-size: auto 12rpx;
  width: 100%;
  display: inline-block;
  position: absolute;
  bottom: -10rpx;
  left: 0;
  height: 12rpx;
}

.date {
  display: inline-block;
  width: 320rpx;
  height: 65rpx;
  margin: 24rpx auto 24rpx -32rpx;
  background: url(data:image/png;base64,) left 4rpx no-repeat;
  background-size: 320rpx 65rpx;
  font-size: 24rpx;
  line-height: 65rpx;
  color: #fff;
  text-indent: 12rpx;
}

.text-1 {
  font-size: 30rpx;
}

.text-2 {
  font-size: 30rpx;
}

.text-2	.number {
  font-size: 60rpx;
  display: inline-block;
}

.address {
  font-size: 24rpx;
  background: url(data:image/png;base64,) left center no-repeat;
  background-size: contain;
  height: 24rpx;
  line-height: 28rpx;
  text-indent: 36rpx;
  color: #999;
  margin: 52rpx 0;
  position: relative;
  top: -10rpx;
}

spin {
  margin: 24rpx auto;
}