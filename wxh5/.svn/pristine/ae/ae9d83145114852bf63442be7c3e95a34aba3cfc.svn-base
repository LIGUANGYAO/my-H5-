view {
  box-sizing: border-box;
}
.map_container {
  position: absolute;
  top: 0;
  bottom: 0px;
  left: 0;
  right: 0;
}

.map {
  width: 100%;
  height: 100%;
}

.go-back {
  position: absolute;
  width: 95rpx;
  height: 95rpx;
  top: 40rpx;
  left: 40rpx;
  z-index: 5;
  text-align: center;
  line-height: 95rpx;
  border-radius: 50%;
  color: #fff;
  background: rgba(0, 0, 0, 0.4);
  display: none;
}

.go-back image {
  margin-top: 30rpx;
  width: 30rpx;
  height: 40rpx;
}

.footer {
  width: 100%;
  height: 80rpx;
  position: fixed;
  bottom: 80rpx;
  left: 0;
  z-index: 1000;
}

.footer-content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-basis: 100%;
  padding: 0 20rpx 20rpx 20rpx;
}

.item {
  display: block;
}

.item-location, .item-refresh {
  width: 80rpx;
  height: 80rpx;
}

image {
  width: 80rpx;
  height: 80rpx;
}

.item-nearby-equipment {
  width: 240rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #f9850a;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  border-radius: 50px;
}

.item-nearby-equipment .icon-activity {
  position: relative;
  top: 4rpx;
  margin-right: 6px;
  font-size: 46rpx;
}

.current-dot-footer {
  position: fixed;
  bottom: 0;
  left: 0;
  flex: 1;
  width: 100%;
  background: #fff;
  box-shadow: 0 -2rpx 5rpx #d5d2cb;
  z-index: 10;
  height: 30%;
}

.go {
  position: relative;
  top: -18rpx;
  width: 100%;
}

.go-btn {
  position: absolute;
  right: 50rpx;
  top: 18rpx;
  width: 90rpx;
  height: 90rpx;
}

.go-btn image {
  width: 90rpx;
  height: 90rpx;
}

.dot {
  display: flex;
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
}

.dot-img {
  width: 108rpx;
  height: 108rpx;
}

.dot-img image {
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
}

.dot-address {
  width: 100%;
  padding-left: 30rpx;
}

.h3 {
  font-size: 32rpx;
  line-height: 46rpx;
  font-weight: normal;
  color: #333;
  height: 40rpx;
}

.p {
  color: #989898;
  font-size: 30rpx;
  padding-top: 8rpx;
  line-height: 150%;
}

.equipment-status {
  padding: 30rpx;
}

.status {
  font-size: 36rpx;
  display: inline-block;
  padding-right: 40rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #a8a8a8;
}

.a {
  width: 136rpx;
  height: 56rpx;
  line-height: 56rpx;
  font-size: 26rpx;
  display: inline-block;
  margin-left: 30rpx;
  padding-bottom: 10rpx;
  text-align: center;
  border-radius: 4rpx;
}

.on-line, .off-line {
  color: #999;
  border: 1rpx solid #ccc;
}

.off-line {
  margin-top: 8rpx;
  margin-left: 60rpx;
}

.line {
  color: #f9850a;
  border: 1px solid #f9850a;
}

.nearbyModal {
  width: 750rpx;
  height: 50%;
  position: absolute;
  bottom: 0;
  z-index: 9996;
  overflow-y: auto;
}

.nearbyModal-container {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9999;
  background: #fff;
  height: 50%;
  border-top: 2rpx solid #f8860a;
}

.container-header {
  width: 750rpx;
  padding: 20rpx;
}

.search {
  position: relative;
}

.search image {
  position: absolute;
  width: 28rpx;
  height: 34rpx;
  top: 24rpx;
  left: 20rpx;
  color: #c5c5c5;
}

.search-input {
  display: flex;
  justify-content: space-between;
}

.search-input  input {
  width: 490rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 70rpx;
  border: none;
  background: #ddd;
  font-size: 26rpx;
  color: #999;
}

.cancel {
  width: 130rpx;
  height: 80rpx;
  color: #fff;
  background-color: #f9850a;
  text-align: center;
  font-size: 34rpx;
  line-height: 80rpx;
  display: block;
}

.container-body {
  height: 80%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  padding-bottom: 24rpx;
}

.ul {
  padding: 2rpx 0;
}

.tip {
  width: 690rpx;
  padding-top: 30rpx;
  text-align: center;
  color: #fff;
}

.li {
  display: flex;
  align-items: center;
  padding: 19rpx 30rpx;
  border-bottom: 1px solid #f2f2f2;
}

.item-content {
  width: 582rpx;
  padding-right: 30rpx;
}

.item-content .h3 {
  display: flex;
  align-items: center;
}

.item-content  .adress {
  color: #333;
  font-size: 30rpx;
}

.item-content  .on-line {
  width: 60rpx;
  height: 40rpx;
  display: block;
  background: #f9850c;
  color: #fff;
  margin-right: 10rpx;
  font-size: 20rpx;
  text-align: center;
  line-height: 40rpx;
  border: none;
}

.item-content  .off-line {
  width: 60rpx;
  height: 40rpx;
  background: #ccc;
  color: #f6f6f6;
  margin-right: 10rpx;
  text-align: center;
  font-size: 14rpx;
  line-height: 40rpx;
}

.p2 {
  font-size: 26rpx;
  padding-top: 10rpx;
  color: #999;
  line-height: 150%;
}

.container-body image {
  width: 118rpx;
  height: 118rpx;
}

.load {
  margin-top: 30rpx;
  text-align: center;
  color: #fff;
}
